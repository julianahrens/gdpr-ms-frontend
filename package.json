{
  "name": "gdpr-management-system-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --watch=false --codeCoverage=true --browsers=ChromeHeadlessCustom",
    "test:watch": "ng test --codeCoverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && npm run test && npm run build:prod",
    "update": "ng update --all=true --createCommits=true --force=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "~10.1.0",
